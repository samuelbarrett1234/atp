{
	"name" : "Propositional Logic",
	
	"definitions" : [
		{ "name" : "true", "arity" : 0 },
		{ "name" : "false", "arity" : 0 },
		{ "name" : "and", "arity" : 2 },
		{ "name" : "or", "arity" : 2 },
		{ "name" : "not", "arity" : 1 },
		{ "name" : "implies", "arity" : 2 }
	],
	
	"axioms" : [
		"not(true) = false",
		"not(false) = true",
		
		"and(true, true) = true",
		"and(false, A) = false",
		"and(A, B) = and(B, A)",
		
		"or(A, B) = not(and(not(A), not(B)))",
		
		"implies(A, B) = or(not(A), B)",
		
		"or(A, not(A)) = true",
		"and(A, not(A)) = false"
	]
}
