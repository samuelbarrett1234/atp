{
	"name" : "An example schema",
	"desc" : "Example equational logic theorem database (without data for \"benefit heuristic\" estimation).",
	"logic" : "EQUATIONAL",
	"target" : "../Data/TheoremDBs/ExampleDB/",
	
	"tables" :
	[
		{
			"name" : "group-theory-thms",
			"context" : "../Data/Definitions/group_theory.json",
			
			"indices" :
			[
				{
					"type" : "hash",
					"key" : "thm-id",
					"filename" : "group-theory-thms.bin"
				}
			]
			
			"columns" :
			[
				{
					"name" : "thm-id", "type" : "uint", "unique" : true, "auto-key" : true
				},
				{ "name" : "thm", "type" : "statement" },
				{ "name" : "proof-completion-state", "type" : "int" },
				{ "name" : "proof-time", "type" : "float" },
				{ "name" : "proof-max-mem", "type" : "int" },
				{ "name" : "proof-num-expansions", "type" : "int" }
			]
		}
	]
}
